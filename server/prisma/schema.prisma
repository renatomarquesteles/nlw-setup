// File generated by `npx prisma init --datasource-provider SQLite`

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Habit {
  id         String   @id @default(uuid()) // Auto sets a primary key with an unique id
  title      String
  created_at DateTime

  @@map("habits")
}
